{
  "editor.rulers": [80],
  "editor.tabSize": 2,

  "files.exclude": {
    "**/cache/": true,
    "**/dist/": true,
    "**/node_modules/": true,
    "**/target/": true,
    "examples/**/gleam/build/": true,
    "src/gleam/**/build/": true
  },

  "rust-analyzer.linkedProjects": [
    "examples/dicom_anonymize/rust/Cargo.toml",
    "examples/dicom_json_read/rust/Cargo.toml",
    "examples/dicom_json_write/rust/Cargo.toml",
    "examples/dicom_read/rust/Cargo.toml",
    "examples/dicom_read_pixel_data/rust/Cargo.toml",
    "examples/dicom_stream/rust/Cargo.toml",
    "examples/dicom_write/rust/Cargo.toml",
    "src/rust/Cargo.toml"
  ],

  "cSpell.enabledFileTypes": { "gleam": true },
  "cSpell.words": [
    "anonymization",
    "bigi",
    "byteorder",
    "charls",
    "charset",
    "charsets",
    "codepoint",
    "codepoints",
    "concat",
    "dcmfx",
    "deque",
    "dicom",
    "dicoms",
    "endianness",
    "gleeunit",
    "hounsfield",
    "ints",
    "jpegls",
    "libc",
    "libjpeg",
    "libjxl",
    "lut",
    "luts",
    "msvc",
    "multiframe",
    "openjpeg",
    "openjph",
    "println",
    "rngs",
    "rposition",
    "seedable",
    "signedness",
    "simplifile",
    "softcopy",
    "submatches",
    "uids",
    "walkdir"
  ],
  "files.associations": {
    "jpeglib12.h": "c"
  }
}
